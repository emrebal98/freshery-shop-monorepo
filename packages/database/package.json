{
  "name": "database",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup --watch",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
    "db:generate": "prisma generate",
    "db:push": "prisma db push --skip-generate"
  },
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "tsup": "^6.5.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.12",
    "prisma": "^4.9.0",
    "eslint": "7.32.0",
    "tsconfig": "workspace:*"
  }
}
